{
  "name": "boomer",
  "version": "0.0.1",
  "description": "A Discord bot for some games.",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/minhducsun2002/boomer"
  },
  "scripts": {
    "transpile": "ttsc",
    "start": "node dist/index.js",
    "start:dev": "ts-node --compiler ttypescript src/index.ts",
    "build": "rimraf dist/ && npm run transpile",
    "build:test": "tsc --noEmit",
    "dev": "ts-node-dev --compiler ttypescript --respawn --no-notify --prefer-ts --transpile-only src/index.ts",
    "lint": "eslint src/**/* --ext .ts"
  },
  "author": "Minh Duc Vo <minhducsun2002@gmail.com>",
  "license": "CC-BY-4.0",
  "dependencies": {
    "@minhducsun2002/node-osr-parser": "0.0.1-1",
    "@minhducsun2002/paged-embeds": "0.1.0",
    "@octokit/rest": "^16.43.1",
    "axios": "^0.19.2",
    "chalk": "^3.0.0",
    "cheerio": "^1.0.0-rc.3",
    "csprng": "^0.1.2",
    "discord-akairo": "^8.0.0",
    "discord.js": "^12.0.1",
    "dotenv": "^8.2.0",
    "escape-string-regexp": "^2.0.0",
    "mongoose": "^5.7.11",
    "mongoose-cache": "^0.1.5",
    "nconf": "^0.10.0",
    "ojsama": "^1.2.5",
    "strip-ansi": "^6.0.0",
    "timezonecomplete": "^5.11.0"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.16",
    "@types/csprng": "^0.1.1",
    "@types/mongoose": "^5.5.34",
    "@types/nconf": "^0.10.0",
    "@types/node": "^12.12.17",
    "@types/ws": "^7.2.1",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "eslint": "^7.4.0",
    "rimraf": "^3.0.2",
    "ts-node": "^8.5.4",
    "ts-node-dev": "^1.0.0-pre.49",
    "ttypescript": "^1.5.10",
    "typescript": "^3.9.6"
  },
  "engines": {
    "node": ">=13.8.0"
  }
}
